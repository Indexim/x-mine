				</a>
			</div>
			<!--navigation-->
			<ul class="metismenu" id="menu">
				@foreach (var menu in ViewBag.Menu)
				{
					if (menu.type == "General" && menu.hidden == "false")
					{
						<li>
							@if (menu.new_tab == "true")
							{
								<a target="_blank" href="@Url.Action((string)menu.link_function, (string)menu.link_controller)" class="has-arrow">
									<div class="parent-icon icon-color-1"><i class="bx bx-home-alt"></i></div>
									<div class="menu-title">@menu.title</div>
								</a>
							}
							else
							{
								<a href="@Url.Action((string)menu.link_function, (string)menu.link_controller)" class="has-arrow">
									<div class="parent-icon icon-color-1"><i class="bx bx-home-alt"></i></div>
									<div class="menu-title">@menu.title</div>
								</a>
							}
						</li>
					}
				}

				@if (ViewBag.MenuRosterCount > 0)
				{
					<li>
						<a href="javascript:;" class="has-arrow">
							<div class="parent-icon icon-color-2"><i class="bx bx-atom"></i></div>
							<div class="menu-title">Roster</div>
						</a>
						<ul>
							@foreach (var menu in ViewBag.Menu)
							{
								if (menu.type == "Roster" && menu.hidden == "false")
								{
									<li><a href="@Url.Action((string)menu.link_function, (string)menu.link_controller)"><i class="bx bx-right-arrow-alt"></i>@menu.title</a></li>
								}
							}
						</ul>
					</li>
				}

				@if (ViewBag.MenuMasterCount > 0)
				{
					<li>
						<a href="javascript:;" class="has-arrow">
							<div class="parent-icon icon-color-3"><i class="bx bx-folder"></i></div>
							<div class="menu-title">Master</div>
						</a>
						<ul>
							@foreach (var menu in ViewBag.Menu)
							{
								if (menu.type == "Master" && menu.hidden == "false")
								{
									<li><a href="@Url.Action((string)menu.link_function, (string)menu.link_controller)"><i class="bx bx-right-arrow-alt"></i>@menu.title</a></li>
								}
							}
						</ul>
					</li>
				}
